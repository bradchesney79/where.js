{
  "name": "where.js",
  "version": "0.0.0",
  "description": "data-driven test method for js frameworks (mocha, jasmine, qunit, tape)",
  "homepage" : "https://github.com/dfkaye/where.js",
  "author": {
    "name": "david kaye (@dfkaye)",
    "email" : "dfkaye@yahoo.com"
  },
  "devDependencies": {
    "jasmine-node": "1.x.x"
  },
  "license": "MIT",
  "main": "./where.js",
  "peerDependencies": {
    "chai": ">= 1.x",
    "expect.js": ">= 0.2",
    "mocha": ">= 1.7",
    "qunitjs": ">= 1.13",
    "qunit-tap": ">= 1.4",
    "should": ">= 3.0.1",
    "tape": ">= 2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dfkaye/where.js.git"
  },
  "scripts": {
    "test": "jasmine-node --verbose ./test/where/where.spec.js",
    "jasmine": "jasmine-node --verbose --matchall ./test/jasmine/where-spec",
    "mocha": "node ./test/mocha/node-suite.js",
    "qunit": "node ./test/qunit/node-suite.js",
    "tape": "node ./test/tape/node-suite.js",
    "testem": "testem -l where -t ./test/where/browser-suite.html",
    "testem-jasmine": "testem -l jasmine -t ./test/jasmine/browser-suite.html",
    "testem-mocha": "testem -l mocha -t ./test/mocha/browser-suite.html",
    "testem-qunit": "testem -l qunit -t ./test/qunit/browser-suite.html",
    "testem-tape": "testem -l tape -t ./test/tape/browser-suite.html"    
  },
  "testling": {
    "scripts": [
      "./vendor/jasmine-2.0.0/jasmine.js",
      "./vendor/jasmine-2.0.0/jasmine-html.js",
      "./vendor/jasmine-2.0.0/boot.js",
      "./where.js",
      "./test/where/where.spec.js",
      "./vendor/jasmine-tap/jasmine-tap.js",      
      "./vendor/jasmine-tap/runner.js"      
    ],
    "harness": "mocha",
    "browsers" : [
      "ie/6..latest",
      "chrome/latest",
      "firefox/latest",
      "safari/latest",
      "opera/latest",
      "iphone/6", 
      "ipad/6"
    ]
  }  
}